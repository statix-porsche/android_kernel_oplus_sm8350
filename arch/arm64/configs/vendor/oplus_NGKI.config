# Alert Slider
CONFIG_IST_DOWN=y
CONFIG_IST_UP=y
CONFIG_MXM_DOWN=y
CONFIG_MXM_UP=y
CONFIG_OPLUS_TRIKEY_HALL=y
CONFIG_OPLUS_TRIKEY_MAIN=y
CONFIG_OPLUS_TRI_STATE_KEY=y

# Architectures
# CONFIG_ARCH_SUNXI is not set
# CONFIG_ARCH_HISI is not set
# CONFIG_ARCH_SHIMA is not set
# CONFIG_ARCH_YUPIK is not set

# Audio
CONFIG_OPLUS_FEATURE_MM_FEEDBACK=n

# Compression
CONFIG_CRYPTO_LZ4=y

# Display
CONFIG_DRM_OPLUS_NOTIFY=y
CONFIG_DRM_OPLUS_PANEL_NOTIFY=y
CONFIG_INPUT_TOUCHSCREEN=y
CONFIG_TOUCHIRQ_UPDATE_QOS=y
CONFIG_TOUCHPANEL_CUSTOM=y
CONFIG_TOUCHPANEL_FOCAL=y
CONFIG_TOUCHPANEL_FOCAL_FT3518=y
CONFIG_TOUCHPANEL_FOCAL_FT3658U=y
CONFIG_TOUCHSCREEN_FTS=n
CONFIG_TOUCHPANEL_NOTIFY=y
CONFIG_TOUCHPANEL_OPLUS=y
CONFIG_TOUCHPANEL_SAMSUNG=y
CONFIG_TOUCHPANEL_SAMSUNG_S6SY792=y
CONFIG_TOUCHPANEL_SYNAPTICS=y
CONFIG_TOUCHPANEL_SYNAPTICS_TCM_ONCELL=y
CONFIG_TOUCHSCREEN_ST=n
CONFIG_TOUCHPANEL_LATE_INIT=y

# Fingerprint
CONFIG_OPLUS_FINGERPRINT=y
CONFIG_OPLUS_FINGERPRINT_COMMON=y
CONFIG_OPLUS_FINGERPRINT_QCOM=y
CONFIG_OPLUS_FINGERPRINT_GOODIX_OPTICAL=y
CONFIG_OPLUS_FINGERPRINT_JIIOV_OPTICAL=y
CONFIG_OPLUS_SECURE=y
CONFIG_OPLUS_SECURE_COMMON=y
CONFIG_OPLUS_SECURE_QCOM=y

# Haptics
CONFIG_AW8697_HAPTIC=y

# MM
CONFIG_MEMORY_HOTPLUG=n

# Misc
CONFIG_OPLUS_FEATURE_ANDROIDBOOT=y
CONFIG_OPLUS_FEATURE_BUILDVARIANT=y
CONFIG_OPLUS_FEATURE_CDT=y
CONFIG_OPLUS_FEATURE_CHARGERPRESENT=y
CONFIG_OPLUS_FEATURE_CMDLINE=y
CONFIG_OPLUS_FEATURE_FTMMODE=y
CONFIG_OPLUS_FEATURE_OPROJECT=y
CONFIG_OPLUS_FEATURE_PROJECTINFO=y
CONFIG_OPLUS_FEATURE_SIMCARDNUM=y
CONFIG_OPLUS_DEVICE_IFNO=y

# NFC
CONFIG_NFC_QTI_I2C=y

# Power
CONFIG_OPLUS_CALL_MODE_SUPPORT=y
CONFIG_OPLUS_CHECK_CHARGERID_VOLT=y
CONFIG_OPLUS_CHG=y
CONFIG_OPLUS_CHIP_SOC_NODE=y
CONFIG_OPLUS_SHIP_MODE_SUPPORT=y
CONFIG_OPLUS_SHORT_C_BATT_CHECK=y
CONFIG_OPLUS_SHORT_USERSPACE=y
CONFIG_OPLUS_SM8350_CHARGER=y
CONFIG_OPLUS_SMART_CHARGER_SUPPORT=y

# Sensors
CONFIG_OPLUS_FEATURE_FEEDBACK=y
CONFIG_OPLUS_FEATURE_SENSOR_CFG=y
CONFIG_OPLUS_SENSOR_FB_QC=n

# Stability
CONFIG_OPLUS_BUG_STABILITY=n

# UFS
CONFIG_OPLUS_FEATURE_UFS=y
CONFIG_OPLUS_FEATURE_UFS_DRIVER=y
CONFIG_SCSI_UFS_BSG=y

# VM
CONFIG_HVC_HAVEN=n

# WiFi
CONFIG_CNSS_QCA6490=y
CONFIG_FRAME_WARN=0
CONFIG_OPLUS_FEATURE_WIFI_BDF=n
CONFIG_OPLUS_FEATURE_WIFI_MAC=n
CONFIG_QCA_CLD_WLAN=y

# Whitelist
CONFIG_UNUSED_KSYMS_WHITELIST="android/abi_gki_aarch64_qcom android/abi_gki_aarch64_qcom_internal android/abi_gki_aarch64_oplus_internal_whitelist android/abi_gki_aarch64_oplus_whitelist"
